#######################################
# flash
#######################################
flash:
	-openocd -f interface/stlink-v2-1.cfg -f target/stm32f4x.cfg -c init -c "reset halt" -c "flash write_image erase $(BUILD_DIR)/$(TARGET).bin 0x08000000" -c "reset" -c "exit"
